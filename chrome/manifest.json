{
  "manifest_version": 3,
  "name": "hrishix6/HttpParrot",
  "description": "Http client for API testing.",
  "version": "1.0.0",
   "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "host_permissions": ["https://*/*", "http://*/*"],
  "permissions": ["storage", "tabs","declarativeNetRequest","declarativeNetRequestWithHostAccess","declarativeNetRequestFeedback"]
}
