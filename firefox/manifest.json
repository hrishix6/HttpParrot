{
    "manifest_version": 2,
    "name": "hrishix6/HttpParrot",
    "description": "Http client for API testing.",
    "homepage_url": "https://github.com/hrishix6/HttpParrot",
    "version": "1.0.0",
     "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png"
        }
    },
    "browser_specific_settings": {
        "gecko": {
          "id": "hrishix6@httpParrot.com",
          "strict_min_version": "42.0"
        }
      },      
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["<all_urls>", "tabs","storage", "unlimitedStorage","webRequest","webRequestBlocking"]
  }
    